<div class="adminPage">
    
    @await Html.PartialAsync("AdminButtonMenuPartial")
        
    <div class="adminHeader">
        
        <h3>Удаление объекта прошло  успешно!</h3>
        <h4>Данные удаленного объекта:</h4>
        <div>
            <div class="padding_bottom_2 displayFlex">
                Название: @Model.Name
            </div>
            <div class="padding_bottom_2 displayFlex">
                Краткое описание: @Model.ShortDescription
            </div>
            <div class="padding_bottom_2 displayFlex">
                Полное описание: @Model.FullDescription
            </div>
            <div class="padding_bottom_2 displayFlex">
                Титульная картинка: @Model.TitleImagePath
            </div>
            <div class="padding_bottom_2 displayFlex">
                Время создания: @Model.DateAdded
            </div>
        </div>
     </div>
</div>
