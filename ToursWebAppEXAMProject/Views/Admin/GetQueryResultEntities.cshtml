@{
   <div class="adminPage">
        <div class="adminButton">
            <button class="">
                <a class="" asp-area="" asp-controller="Admin" asp-action="">Резерв</a>
             </button>
             <button class="">
                <a class="" asp-area="" asp-controller="Admin" asp-action="">Резерв 2</a>
             </button>
             <button class="">
                 <a class="" asp-area="" asp-controller="Admin" asp-action="">Резерв 3"</a>
             </button>
             <button class="">
                 <a class="" asp-area="" asp-controller="Admin" asp-action="">Резерв 4</a>
             </button>
        </div>
        <div class="adminHeader">
            @foreach(var item in @Model)
            {
                <div>
                    <div class="padding_bottom_2">
                        <h2>@item.Name</h2>
                    </div>
                    <div class="padding_bottom_2">
                        @*<h4>@item.ShortDescription</h4>*@
                    </div>
                    <div>
                        <a asp-area="" asp-controller="Admin" asp-action="EditItem" asp-route-id="@item.Id">редактировать</a>	
                        <a asp-area="" asp-controller="Admin" asp-action="DeleteItem" asp-route-id="">удалить</a>
                    </div>
                </div>
            }
        </div>
   </div>
}