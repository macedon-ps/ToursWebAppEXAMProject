@model Product

 <div class="homePage">
     
     @await Html.PartialAsync("SearchButtonMenuPartial")

     <div class="searchHeaderOne">
        <div class="titleImage50">
            <img src="@Model.TitleImagePath" alt="@Model.TitleImagePath" align="left">
            @*<p>Расположение фото: </p>
            <h3>@Model.TitleImagePath</h3>*@
        </div>
        <div>
            <h2>@Model.Name</h2>
        </div>
        <div>
            @Html.Raw(Model.FullDescription)
        </div>
        <div class="displayFlexRow">
            <h4>Id турпродукта: @Model.Id</h4>
            <h4>Дата: @Model.DateAdded</h4>
        </div>
    </div>
</div>

